{
    "name": "kryptamine/groot",
    "description": "PHP 8 attributes based router adapter",
    "keywords": [
        "router",
        "php-attributes"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Kryptamine\\Groot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kryptamine\\Groot\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Alexander Satretdinov",
            "email": "satretdinovalexander@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
